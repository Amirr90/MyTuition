<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="dashboard1"
            type="com.mytuition.models.DashboardModel1" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        android:paddingBottom="5dp">




        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/cv1"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="2dp"
            android:background="@drawable/border_rectangle_with_white_bg"
            android:clickable="true"
            android:focusable="true"
            android:paddingBottom="@dimen/_10sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </LinearLayout>
        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="68dp"
            android:layout_height="78dp"
            android:layout_marginStart="10dp"
            android:elevation="10dp"
            app:layout_constraintBottom_toTopOf="@+id/cv1"
            app:layout_constraintStart_toStartOf="@+id/cv1"
            app:layout_constraintTop_toTopOf="@+id/cv1" />
        <TextView
            android:id="@+id/textView55"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:elevation="10dp"
            android:fontFamily="@font/poppins"
            android:paddingEnd="10dp"
            android:text="find_teacher_by"
            android:textColor="@color/colorPrimary"
            android:textSize="16sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/imageView21"
            app:layout_constraintTop_toBottomOf="@+id/imageView21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="10dp"
            android:fontFamily="@font/poppins_bold"
            android:paddingRight="5dp"
            android:text="@{dashboard1.title}"
            android:textColor="@color/colorPrimary"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView55"
            app:layout_constraintTop_toBottomOf="@+id/textView55" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>


